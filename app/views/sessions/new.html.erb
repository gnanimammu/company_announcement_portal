<h1>User Registration Form if not Register, please Register your name </h1>
<%= link_to 'Register', new_user_path(@user) %>
<h1>Login</h1>
<%= form_with url: session_path, method: :post do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit 'Login' %>
  </div>
<% end %>
